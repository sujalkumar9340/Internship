package Araaysprts;

import java.util.Scanner;

public class Table {
public static void main(String[] args) {
	Scanner cs=new Scanner(System.in);
	System.out.println("enter the number to print the table");
	int number=cs.nextInt();
	for(int i=1;i<=10;i++) {
		System.out.println(number*i);
	}
	
} 
}
